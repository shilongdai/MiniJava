class TestStatement {

    private static void main(int[] argv, int argc) {
        int[] testRefAssign = argv;
        testRefAssign[1] = 2;
        while(argc != 5)
            argc = argc + 1;
        if (1 == 1) {
            int two = 1 + 1;
            gcd(argv[0], argv[1]);
        } else {
            if(2 == 2) {
                sneed();
            } else {
                feed();
            }
        }
        {
            doesStuff(1, 2, 3, 5+5);
            {
                doMoreStuf();
            }
        }
    }

    private static int gcd(int x, int y) {
        while(x != y) {
            if(x > y)
                x = x - y;
            else
                y = y - x;
        }
        return x;
    }

}